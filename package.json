{
  "name": "kepler-launch",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "install-server": "pnpm i --prefix server",
    "install-client": "pnpm i --prefix client",
    "install": "concurrently \"pnpm run install-client\"  \"pnpm run install-server\"",
    "server": "pnpm watch --prefix server",
    "client": "pnpm start --prefix client",
    "dev": "concurrently \"pnpm run server\"  \"pnpm run client\"",
    "deploy": "npm run build --prefix client && pnpm start --prefix server",
    "deploy-cluster": "npm run build --prefix client && pnpm run cluster --prefix server",
    "test": "pnpm test --prefix server && pnpm test --prefix client"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/clinzy25/kepler-launch.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/clinzy25/kepler-launch/issues"
  },
  "homepage": "https://github.com/clinzy25/kepler-launch#readme",
  "dependencies": {
    "concurrently": "^6.2.0"
  }
}
